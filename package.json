  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "esbuild": "^0.15.16",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  }
}
